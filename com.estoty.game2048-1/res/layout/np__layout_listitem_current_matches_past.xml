    <ImageView android:id="@id/np__current_matches_past_your_rank_view" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/np__current_matches_list_ribbon_flag_margin" android:src="@drawable/np__ic_current_matches_rank_ribbon_flag_orange" android:layout_below="@id/np__current_matches_past_ribbon_line" android:layout_alignParentLeft="true" android:contentDescription="@string/np__application_name" />
    <ImageView android:gravity="top|center" android:id="@id/np__current_matches_past_your_rank_star" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="@dimen/np__current_matches_list_item_margin" android:src="@drawable/np__ic_current_matches_rank_ribbon_star" android:layout_alignLeft="@id/np__current_matches_past_your_rank_view" android:layout_alignRight="@id/np__current_matches_past_your_rank_view" android:layout_alignParentTop="true" android:contentDescription="@string/np__application_name" />
