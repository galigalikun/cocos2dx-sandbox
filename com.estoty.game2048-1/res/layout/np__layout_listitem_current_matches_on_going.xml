    <ImageView android:gravity="center_vertical" android:id="@id/np__current_matches_on_going_third_player_thumbnail" android:layout_width="@dimen/np__current_matches_list_player_thumbnail_size" android:layout_height="@dimen/np__current_matches_list_player_thumbnail_size" android:layout_marginLeft="@dimen/np__current_matches_list_item_margin" android:layout_marginTop="@dimen/np__current_matches_list_item_margin" android:src="@color/np__nextpeer_orange" android:layout_toRightOf="@id/np__current_matches_on_going_second_player_thumbnail" android:layout_alignTop="@id/np__current_matches_on_going_your_rank_view" android:contentDescription="@string/np__application_name" style="@style/NP__.Widget.Replacement" />
